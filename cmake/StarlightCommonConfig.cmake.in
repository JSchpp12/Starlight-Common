@PACKAGE_INIT@

# If Starlight::StarlightCommon already exists (e.g., because the source
# project was added via add_subdirectory in a superbuild), return early.
if (TARGET Starlight::StarlightCommon)
    set(StarlightCommon_FOUND TRUE)
    return()
endif()

# Otherwise, load the installed targets. This file is generated by install(EXPORT ...)
include("${CMAKE_CURRENT_LIST_DIR}/StarlightCommonTargets.cmake")

# Optional variables for consumers
set(StarlightCommon_FOUND TRUE)
set(StarlightCommon_VERSION "@PROJECT_VERSION@")